# OAuth - GitHub
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=

# GitHub App (create at github.com/settings/apps/new)
# Required permissions: Contents: Read, Pull requests: Read, Metadata: Read
# Required events: installation, pull_request
GITHUB_APP_ID=your_app_id
GITHUB_APP_PRIVATE_KEY="-----BEGIN RSA PRIVATE KEY-----\n...\n-----END RSA PRIVATE KEY-----"
GITHUB_APP_WEBHOOK_SECRET=your_webhook_secret

# AI - nanoGPT
NANOGPT_API_KEY=

# Billing - Polar.sh
POLAR_WEBHOOK_SECRET=
POLAR_HACKER_PRODUCT_ID=
POLAR_HACKER_CHECKOUT_LINK=
POLAR_ACCESS_TOKEN=

# Encryption (generate with: openssl rand -hex 32)
ENCRYPTION_KEY=

# Web Push VAPID keys (generate with: npx web-push generate-vapid-keys)
VAPID_PUBLIC_KEY=
VAPID_PRIVATE_KEY=
